{
    "name": "spatie/laravel-medialibrary-v6-to-v7-filesystem-upgrade",
    "description": "This tool will add the original name to the conversions like specified in the upgrade guide of the medialibrary version 7",
    "keywords": [
        "spatie",
        "laravel-medialibrary-v6-to-v7-filesystem-upgrade"
    ],
    "homepage": "https://github.com/spatie/laravel-medialibrary-v6-to-v7-filesystem-upgrade",
    "license": "MIT",
    "authors": [
        {
            "name": "Thomas Verhelst",
            "email": "tvke91@gmail.com",
            "homepage": "https://spatie.be",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.1",
        "illuminate/console": "~5.5.0|~5.6.0|~5.7.0",
        "illuminate/support": "~5.5.0|~5.6.0|~5.7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.0",
        "orchestra/testbench" : "^3.6",
        "spatie/laravel-medialibrary" : "^6.0"
    },
    "autoload": {
        "psr-4": {
            "Spatie\\MedialibraryV7UpgradeTool\\" : "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spatie\\MedialibraryV7UpgradeTool\\Tests\\" : "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"

    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "laravel": {
            "providers": [
                "Spatie\\MedialibraryV7UpgradeTool\\UpgradeToolServiceProvider"
            ]
        }
    }
}
